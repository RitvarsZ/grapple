{
  "name": "Grapple",
  "description": "Quicky search and open bookmarks using fuzzy search.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "bookmarks"
  ],
  "action": {
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open-grapple-overlay": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Open the Grapple overlay"
    }
  }
}
